  object QryMapeoSAP: TQuery
    DatabaseName = 'PCORP'
    SQL.Strings = (
      
        '                              SELECT CCST.F_OPERACION, CCSTD.*, ' +
        'P.NOMBRE, CP.DESC_L_PRODUCTO'
      '                              FROM CR_CONT_GEN_TR_D CCSTD,'
      '                                   PERSONA P,'
      '                                   CR_PRODUCTO CP,'
      
        '                                   ( SELECT FOL_GENERACION, F_OP' +
        'ERACION'
      '                                     FROM CR_CONT_GEN_TRAN'
      
        '                                     WHERE SIT_GENERACION = '#39'GEN' +
        #39
      
        '                                       AND F_OPERACION = TO_DATE' +
        '('#39'14/04/2008'#39','#39'DD/MM/YYYY'#39')'
      '                                   ) CCST,'
      '                                   ( SELECT TEMP.* '
      
        '                                     FROM ( SELECT CCSTD.FOL_GEN' +
        'ERACION,'
      
        '                                                   CCSTD.CVE_MON' +
        'EDA,'
      '                                                   CCE.CUENTA, '
      
        '                                                   CCSTD.CVE_CTA' +
        '_CONTABLE'
      
        '                                            FROM CR_CONT_GEN_TR_' +
        'D CCSTD,'
      '                                                 CR_CREDITO CC,'
      '                                                 CR_CONTRATO CO,'
      '                                                 CONTRATO CTO,'
      
        '                                                 CG_CUENTA_EQUIV' +
        '@CONT CCE,'
      
        '                                                 ( SELECT FOL_GE' +
        'NERACION, F_OPERACION'
      
        '                                                   FROM CR_CONT_' +
        'GEN_TRAN'
      
        '                                                   WHERE SIT_GEN' +
        'ERACION = '#39'GEN'#39
      
        '                                                     AND F_OPERA' +
        'CION = TO_DATE('#39'14/04/2008'#39','#39'DD/MM/YYYY'#39')'
      '                                                 ) CCST'
      
        '                                            WHERE CCSTD.FOL_GENE' +
        'RACION = CCST.FOL_GENERACION'
      
        '                                              AND CC.ID_CREDITO ' +
        '= CCSTD.ID_CREDITO'
      
        '                                              AND CO.ID_CONTRATO' +
        ' = CC.ID_CONTRATO'
      
        '                                              AND CTO.ID_CONTRAT' +
        'O = CC.ID_CONTRATO'
      
        '                                              AND CTO.ID_EMPRESA' +
        ' = 2'
      
        '                                              AND CCE.CVE_CTA_CO' +
        'NTABLE (+)= CCSTD.CVE_CTA_CONTABLE'
      
        '                                            GROUP BY CCSTD.FOL_G' +
        'ENERACION, CCSTD.CVE_MONEDA, CCE.CUENTA, CCSTD.CVE_CTA_CONTABLE'
      '                                           ) TEMP'
      '                                     WHERE TEMP.CUENTA IS NULL'
      '                                   ) CTA_FALTANTES'
      
        '                              WHERE CCSTD.FOL_GENERACION = CCST.' +
        'FOL_GENERACION'
      
        '                                AND CCSTD.CVE_CTA_CONTABLE = CTA' +
        '_FALTANTES.CVE_CTA_CONTABLE'
      
        '                                AND P.ID_PERSONA = CCSTD.ID_ACRE' +
        'DITADO'
      
        '                                AND CP.CVE_PRODUCTO_CRE = CCSTD.' +
        'CVE_PRODUCTO')
    Left = 649
    Top = 124
    object QryMapeoSAPF_OPERACION: TDateTimeField
      DisplayLabel = 'Fecha de Operacion'
      FieldName = 'F_OPERACION'
    end
    object QryMapeoSAPCVE_CTA_CONTABLE: TStringField
      DisplayLabel = 'Cta. Contable  Fatante de MAPEAR'
      FieldName = 'CVE_CTA_CONTABLE'
      Size = 24
    end
    object QryMapeoSAPCVE_MONEDA: TFloatField
      DisplayLabel = 'Moneda'
      FieldName = 'CVE_MONEDA'
    end
  end

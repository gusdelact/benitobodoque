  object QryMovtoSinCta: TQuery
    DatabaseName = 'PCORP'
    SQL.Strings = (
      
        '  SELECT CCCT.*,'#39'<<>>'#39' AS SEP_I ,CCOC.*, '#39'<<>>'#39' AS SEP_II ,JAVI.' +
        '*'
      '  FROM'
      '  (      SELECT CT.Id_Credito,'
      
        '               CT.Id_Transaccion, CT.Cve_Operacion As Cve_Oper_C' +
        'red,'
      '               NVL(CT.Cve_Comision, '#39'NA'#39') AS Cve_Comision,'
      '               CDT.Cve_Concepto,CT.Cve_Tipo_Movto,'
      '               CDT.Imp_Concepto,CT.F_Operacion,'
      '               CT.F_Valor,CT.Sit_Transaccion,'
      
        '               CTO.id_empresa,CRCTO.Cve_Producto_Cre As Cve_Prod' +
        'ucto,'
      '               CTO.Cve_Moneda,'
      
        '               PKGCRREDESCTO.OBTCCOSTO(CT.Id_Credito) AS Id_Cent' +
        '_Costo,'
      '               CTO.Id_Titular as Id_Acreditado,'
      '               '#39'TRA'#39' AS Cve_Origen_tran,'
      '               TO_CHAR(CT.Id_Transaccion) AS Tx_Referencia,'
      '               PER.Cve_Tipo_Sector,'
      '                    PROD.Cve_Ind_Adi_Cont AS Refinanciamiento,'
      
        '                    PROD.Cve_Pr_Contable AS Num_Prod_Contabilida' +
        'd'
      
        '          FROM Cr_Transaccion CT, Persona PER, Cr_Det_Transac CD' +
        'T,'
      
        '               Cr_Credito CRED, Contrato CTO, Cr_Contrato CRCTO,' +
        ' Cr_Producto PROD,'
      
        '               Cr_Operacion COPE, CR_CONT_SIS_PROD SPROD, CR_CON' +
        'CEPTO CCO'
      
        '               /* ROIM 17/04/2006 SE AGREGA CAMPO PARA DESCRIMIN' +
        'ER CONCEPTOS*/'
      '               ,CR_REL_OPE_CON CROC'
      '               /* /ROIM */'
      '         WHERE CDT.Id_Transaccion = CT.Id_Transaccion'
      '           AND CDT.Cve_Operacion  = CT.Cve_Operacion'
      '           AND CT.Id_Credito = CRED.Id_Credito'
      '           AND CRED.Id_Contrato = CTO.Id_Contrato'
      '           AND CRED.Id_Contrato = CRCTO.Id_Contrato'
      '           AND CRED.Fol_Contrato = CRCTO.Fol_Contrato'
      '           AND CRCTO.Cve_Producto_Cre = PROD.Cve_Producto_Cre'
      '           AND CRCTO.Cve_Producto_Cre = SPROD.Cve_Producto_Cre'
      '           AND CT.Cve_Operacion = COPE.Cve_Operacion'
      '           AND SPROD.CVE_CONT_SISTEMA = '#39'CTICRE'#39
      '           AND PROD.B_Genera_Conta = '#39'V'#39
      '           AND CCO.Cve_Concepto = CDT.Cve_Concepto'
      '           AND CCO.b_gen_conta_icre = '#39'V'#39
      '           AND COPE.B_Gen_Conta_Icre = '#39'V'#39
      '           AND CT.Sit_Transaccion = '#39'AC'#39
      '           AND NVL(CT.B_Contabilizado,'#39'F'#39') = '#39'F'#39
      '           AND CTO.id_empresa = 2'
      
        '           AND CT.F_Operacion = TO_DATE('#39'29/04/2008'#39','#39'DD/MM/YYYY' +
        #39')'
      '           AND PER.Id_Persona = CTO.Id_Titular'
      
        '           /* ROIM 17/04/2006 SE AGREGA CAMPO PARA DESCRIMINER C' +
        'ONCEPTOS*/'
      '           AND CROC.CVE_OPERACION = CDT.CVE_OPERACION'
      '           AND CROC.CVE_CONCEPTO = CDT.CVE_CONCEPTO'
      
        '           AND (CROC.B_OMITE_CON_I IS NULL OR CROC.B_OMITE_CON_I' +
        ' = '#39'F'#39')'
      '           /* /ROIM */'
      '         UNION ALL'
      '         SELECT CH.Id_Credito,'
      '               CH.Id_Credito AS Id_Transaccion,'
      '               /*ROIM*/'
      
        '               DECODE(CCE.CVE_TIPO_INTERES,'#39'IN'#39','#39'INPROV'#39',NULL,'#39'P' +
        'ROVIS'#39','#39'PROVIS'#39') AS Cve_Oper_Cred,'
      '               /*ROIM FIN*/'
      '               '#39'NA'#39' AS Cve_Comision, '#39'IMPBRU'#39' AS Cve_Concepto,'
      
        '               DECODE(CH.F_Traspaso_Venc, NULL, '#39'MD'#39', '#39'IV'#39') AS C' +
        've_Tipo_Movto,'
      '               CH.Imp_Interes_Dia AS Imp_Concepto,'
      
        '               CH.F_Cierre AS F_Operacion, CH.F_Cierre AS F_Valo' +
        'r,'
      '               '#39'AC'#39' AS Sit_Transaccion, CTO.id_empresa,'
      '               CRCTO.Cve_Producto_Cre As Cve_Producto,'
      '               CTO.Cve_Moneda,'
      
        '               PKGCRREDESCTO.OBTCCOSTO(CH.Id_Credito) AS Id_Cent' +
        '_Costo,'
      
        '               CTO.Id_Titular as Id_Acreditado,'#39'PRO'#39' AS Cve_Orig' +
        'en_tran,'
      '               TO_CHAR(CH.Id_Credito) AS Tx_Referencia,'
      '               PER.Cve_Tipo_Sector,'
      '                    PROD.Cve_Ind_Adi_Cont AS Refinanciamiento,'
      
        '                    PROD.Cve_Pr_Contable AS Num_Prod_Contabilida' +
        'd'
      '          FROM Cr_Histo_Cred CH, Cr_Credito CRED, Contrato CTO,'
      
        '               Cr_Contrato CRCTO, Persona PER, Cr_Producto PROD,' +
        ' CR_CONT_SIS_PROD SPROD'
      '               /*ROIM*/'
      '               ,(SELECT * FROM CR_CESION) CCE'
      '               /*ROIM FIN*/'
      '         WHERE CH.Id_Credito = CRED.Id_Credito'
      '           AND CRED.Id_Contrato = CTO.Id_Contrato'
      '           AND CRED.Id_Contrato = CRCTO.Id_Contrato'
      '           AND CRED.Fol_Contrato = CRCTO.Fol_Contrato'
      '           AND CRCTO.Cve_Producto_Cre = PROD.Cve_Producto_Cre'
      '           AND CRCTO.Cve_Producto_Cre = SPROD.Cve_Producto_Cre'
      '           AND SPROD.CVE_CONT_SISTEMA = '#39'CTICRE'#39
      '           AND PROD.B_Genera_Conta = '#39'V'#39
      '--           AND NVL(CH.B_Contabilizado,'#39'F'#39') = '#39'F'#39
      '           AND CTO.id_empresa = 2'
      
        '           AND CH.F_Cierre    = TO_DATE('#39'29/04/2008'#39','#39'DD/MM/YYYY' +
        #39')'
      '           AND PER.Id_Persona = CTO.Id_Titular'
      '               /*ROIM*/'
      '           AND CCE.ID_CESION (+)= CRED.Id_Credito'
      '               /*ROIM FIN*/'
      '         UNION ALL'
      '         SELECT CRED.Id_Credito,'
      '               GA.Id_Solicitud AS Id_Transaccion,'
      '               '#39'GARANT'#39' AS Cve_Oper_Cred,'
      '               '#39'NA'#39' AS Cve_Comision,'
      '               '#39'IMPBRU'#39' AS Cve_Concepto,'
      '               '#39'NA'#39' AS Cve_Tipo_Movto,'
      '               GAD.Imp_Garantia AS Imp_Concepto,'
      '               GA.F_Revision_Gar AS F_Operacion,'
      '               GA.F_Revision_Gar AS F_Valor,'
      '               '#39'AC'#39' AS Sit_Transaccion,'
      '               CTO.id_empresa,'
      '               CRCTO.Cve_Producto_Cre As Cve_Producto,'
      '               CTO.Cve_Moneda,'
      
        '               PKGCRREDESCTO.OBTCCOSTO(CRED.Id_Credito) AS Id_Ce' +
        'nt_Costo,'
      '               CTO.Id_Titular as Id_Acreditado,'
      '               '#39'GAR'#39' AS Cve_Origen_tran,'
      '               TO_CHAR(GA.Id_Solicitud) AS Tx_Referencia,'
      '               PER.Cve_Tipo_Sector,'
      '                    PROD.Cve_Ind_Adi_Cont AS Refinanciamiento,'
      
        '                    PROD.Cve_Pr_Contable AS Num_Prod_Contabilida' +
        'd'
      
        '          FROM Cr_Ga_Registro GA, Cr_Ga_Dis_Gar GAD, Cr_Credito ' +
        'CRED,'
      
        '                Contrato CTO, Cr_Contrato CRCTO, Persona PER, Cr' +
        '_Producto PROD, CR_CONT_SIS_PROD SPROD'
      '          WHERE GA.Id_Solicitud = GAD.Id_Reg_Sol_Gar'
      '           AND GAD.Id_Disposicion = CRED.Id_Credito'
      '           AND CRED.Id_Contrato = CTO.Id_Contrato'
      '           AND CRED.Id_Contrato = CRCTO.Id_Contrato'
      '           AND CRED.Fol_Contrato = CRCTO.Fol_Contrato'
      '           AND CRCTO.Cve_Producto_Cre = PROD.Cve_Producto_Cre'
      '           AND CRCTO.Cve_Producto_Cre = SPROD.Cve_Producto_Cre'
      '           AND SPROD.CVE_CONT_SISTEMA = '#39'CTICRE'#39
      '           AND PROD.B_Genera_Conta = '#39'V'#39
      '--           AND NVL(GA.B_Contabilizado,'#39'F'#39') = '#39'F'#39
      '           AND CTO.id_empresa = 2'
      
        '           AND GA.F_Revision_Gar = TO_DATE('#39'29/04/2008'#39','#39'DD/MM/Y' +
        'YYY'#39')'
      '           AND PER.Id_Persona = CTO.Id_Titular'
      
        '         ORDER BY Id_credito, Cve_Origen_tran, Tx_Referencia ) J' +
        'AVI,'
      '  (      SELECT * FROM cr_cont_cnf_tran) CCCT,'
      '  (      SELECT * FROM cr_cont_ope_con) CCOC'
